{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layout/*": ["src/layouts/*"],
      "@images/*": ["src/images/*"],
      "@types/": ["src/types/*"],
      "@data/*": ["src/data/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
